    <Style TargetType="{x:Type Controls:ZoomControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Controls:ZoomControl}">
                    <ControlTemplate.Resources>
                        <SolidColorBrush x:Key="SelectionBrush"
                                         Opacity="0.5"
                                         po:Freeze="True"
                                         Color="LightBlue" />
                        <SolidColorBrush x:Key="AreaSelectionBrush"
                                         Opacity="0.5"
                                         po:Freeze="True"
                                         Color="LightGreen" />
                        <Pen x:Key="SelectionPen"
                             Brush="Black"
                             Thickness="2.0"
                             po:Freeze="True" />
                        <Pen x:Key="ViewportPen"
                             Brush="Red"
                             Thickness="1.0"
                             po:Freeze="True" />
                        <!--
                            <InputBindingCollection x:Key="InputBindings">
                            <KeyBinding Command="zb:Zoombox.ZoomOut" Key="Left" Modifiers="Control" />
                            <KeyBinding Command="zb:Zoombox.ZoomOut" Key="Up" Modifiers="Control" />
                            <KeyBinding Command="zb:Zoombox.ZoomIn" Key="Right" Modifiers="Control" />
                            <KeyBinding Command="zb:Zoombox.ZoomIn" Key="Down" Modifiers="Control" />
                            <KeyBinding Command="zb:Zoombox.PanLeft" Key="Left" Modifiers="Shift+Control" />
                            <KeyBinding Command="zb:Zoombox.PanUp" Key="Up" Modifiers="Shift+Control" />
                            <KeyBinding Command="zb:Zoombox.PanRight" Key="Right" Modifiers="Shift+Control" />
                            <KeyBinding Command="zb:Zoombox.PanDown" Key="Down" Modifiers="Shift+Control" />
                            </InputBindingCollection>
                        -->
                    </ControlTemplate.Resources>
                    <Grid>
                        <!--
                            <Border BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            ClipToBounds="{TemplateBinding ClipToBounds}">
                        -->

                        <Border Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                ClipToBounds="{TemplateBinding ClipToBounds}">

                            <Controls:ZoomContentPresenter x:Name="PART_Presenter" ClipToBounds="False" />
                        </Border>

                        <Border Name="ViewFinder"
                                Width="200"
                                Height="190"
                                Margin="10"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Bottom"
                                Background="#b0666666"
                                BorderBrush="{TemplateBinding Foreground}"
                                BorderThickness="2"
                                CornerRadius="4"
                                Opacity="0"
                                RenderTransformOrigin="1,1">
                            <Border.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform x:Name="LayoutScaleTransform"
                                                    ScaleX="0"
                                                    ScaleY="0" />
                                </TransformGroup>
                            </Border.RenderTransform>
                            <Grid Margin="4">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <StackPanel Margin="0,0,0,3" Orientation="Horizontal">
                                    <Slider Width="100"
                                            Height="10"
                                            HorizontalAlignment="Center"
                                            AutoToolTipPlacement="BottomRight"
                                            AutoToolTipPrecision="1"
                                            IsEnabled="True"
                                            LargeChange="1"
                                            Maximum="{TemplateBinding MaxZoom,
                                                                      Converter={StaticResource log10Converter}}"
                                            Minimum="{TemplateBinding MinZoom,
                                                                      Converter={StaticResource log10Converter}}"
                                            Orientation="Horizontal"
                                            SmallChange="0.1"
                                            TickFrequency="1"
                                            TickPlacement="None"
                                            Ticks="0.1, 0.25, 0.5, 0.75, 1.0, 1.25, 1.5, 2"
                                            Value="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                            Path=Zoom,
                                                            Mode=TwoWay,
                                                            Converter={StaticResource log10Converter}}" />
                                    <Button x:Name="FitButton"
                                            Width="20px"
                                            Height="20px"
                                            Margin="2,0"
                                            Command="Controls:ZoomControl.Fit"
                                            IsEnabled="False"
                                            Style="{StaticResource TransparentButton}"
                                            ToolTip="Fit Content within Bounds">
                                        <Image Margin="2"
                                               Source="{StaticResource FitContentGlyph}"
                                               po:Freeze="true" />
                                    </Button>
                                    <Button x:Name="FillButton"
                                            Width="20px"
                                            Height="20px"
                                            Margin="2,0"
                                            Command="Controls:ZoomControl.Fill"
                                            Style="{StaticResource TransparentButton}"
                                            ToolTip="Fill Bounds with Content">
                                        <Image Margin="2"
                                               Source="{StaticResource FillToBoundsGlyph}"
                                               po:Freeze="true" />
                                    </Button>
                                    <Button x:Name="CenterButton"
                                            Width="20px"
                                            Height="20px"
                                            Margin="2,0"
                                            Command="Controls:ZoomControl.Center"
                                            Style="{StaticResource TransparentButton}"
                                            ToolTip="Center Content">
                                        <Image Margin="2"
                                               Source="{StaticResource CenterGlyph}"
                                               po:Freeze="true" />
                                    </Button>
                                </StackPanel>
                                <ToggleButton x:Name="ToggleViewFinderButton"
                                              Width="13"
                                              Height="13"
                                              Margin="0,2,0,0"
                                              HorizontalAlignment="Right"
                                              VerticalAlignment="Top"
                                              Foreground="White"
                                              IsChecked="{Binding ElementName=ShowViewFinder,
                                                                  Path=IsChecked}"
                                              Style="{StaticResource TransparentButton}"
                                              ToolTip="Hide View Finder">
                                    <Image Margin="2"
                                           Source="{StaticResource CloseXDrawingImage}"
                                           po:Freeze="true" />
                                </ToggleButton>
                                <Border Grid.Row="1"
                                        Background="#80FFFFFF"
                                        BorderBrush="{StaticResource GlyphBrush}"
                                        BorderThickness="2" />
                                <Controls:ViewFinderDisplay x:Name="ViewFinderDisplay"
                                                            Grid.Row="1"
                                                            Margin="2"
                                                            Background="#C0FFFFFF"
                                                            ClipToBounds="True"
                                                            ShadowBrush="#80FFFFFF"
                                                            ViewportBrush="Transparent"
                                                            ViewportPen="{StaticResource ViewportPen}" />
                            </Grid>
                        </Border>
                        <ToggleButton Name="ShowViewFinder"
                                      Width="22"
                                      Height="22"
                                      Margin="5"
                                      HorizontalAlignment="Right"
                                      VerticalAlignment="Bottom"
                                      Background="Transparent"
                                      FocusVisualStyle="{x:Null}"
                                      Foreground="Transparent"
                                      IsChecked="{Binding Path=ViewFinderVisibility,
                                                          Mode=TwoWay,
                                                          RelativeSource={RelativeSource TemplatedParent},
                                                          Converter={StaticResource TrueIfVisibleConverter}}"
                                      IsTabStop="False"
                                      Style="{StaticResource ViewFinderToggleButtonStyle}"
                                      ToolTip="Show View Finder">
                            <Grid Name="ShowViewFinderGlyph" Opacity="0">
                                <Image x:Name="ShowViewFinderGlyphImage"
                                       Source="{StaticResource ShowViewFinderGlyph}"
                                       po:Freeze="true" />
                            </Grid>
                        </ToggleButton>
                        <Canvas>

                            <Border Canvas.Left="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                          Path=ZoomBox.X}"
                                    Canvas.Top="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                         Path=ZoomBox.Y}"
                                    Width="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                    Path=ZoomBox.Width}"
                                    Height="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                     Path=ZoomBox.Height}"
                                    Background="{TemplateBinding ZoomBoxBackground}"
                                    BorderBrush="{TemplateBinding ZoomBoxBorderBrush}"
                                    BorderThickness="{TemplateBinding ZoomBoxBorderThickness}"
                                    Opacity="{TemplateBinding ZoomBoxOpacity}" />
                        </Canvas>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation DecelerationRatio="1"
                                                         Duration="0:0:0.3"
                                                         FillBehavior="HoldEnd"
                                                         From="0"
                                                         Storyboard.TargetName="ShowViewFinderGlyph"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="1" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation DecelerationRatio="1"
                                                         Duration="0:0:0.3"
                                                         FillBehavior="HoldEnd"
                                                         From="1"
                                                         Storyboard.TargetName="ShowViewFinderGlyph"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="0" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger SourceName="ShowViewFinder" Property="IsChecked" Value="False">
                            <Setter TargetName="FitButton" Property="Focusable" Value="False" />
                            <Setter TargetName="FillButton" Property="Focusable" Value="False" />
                            <Setter TargetName="CenterButton" Property="Focusable" Value="False" />
                            <Setter TargetName="ToggleViewFinderButton" Property="Focusable" Value="False" />
                        </Trigger>
                        <EventTrigger RoutedEvent="ToggleButton.Checked" SourceName="ShowViewFinder">
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation DecelerationRatio="1"
                                                     Duration="0:0:0.3"
                                                     FillBehavior="HoldEnd"
                                                     From="0"
                                                     Storyboard.TargetName="ViewFinder"
                                                     Storyboard.TargetProperty="Opacity"
                                                     To="1" />
                                    <DoubleAnimation DecelerationRatio="1"
                                                     Duration="0:0:0.3"
                                                     FillBehavior="HoldEnd"
                                                     From="1"
                                                     Storyboard.TargetName="ShowViewFinder"
                                                     Storyboard.TargetProperty="Opacity"
                                                     To="0" />
                                    <DoubleAnimationUsingKeyFrames AccelerationRatio="1"
                                                                   BeginTime="00:00:00"
                                                                   Storyboard.TargetName="LayoutScaleTransform"
                                                                   Storyboard.TargetProperty="(ScaleTransform.ScaleY)">
                                        <SplineDoubleKeyFrame KeySpline="0.5,0.5,0.5,0.5"
                                                              KeyTime="00:00:00.3"
                                                              Value="1" />
                                    </DoubleAnimationUsingKeyFrames>
                                    <DoubleAnimationUsingKeyFrames AccelerationRatio="1"
                                                                   BeginTime="00:00:00"
                                                                   Storyboard.TargetName="LayoutScaleTransform"
                                                                   Storyboard.TargetProperty="(ScaleTransform.ScaleX)">
                                        <SplineDoubleKeyFrame KeySpline="0.5,0.5,0.5,0.5"
                                                              KeyTime="00:00:00.3"
                                                              Value="1" />
                                    </DoubleAnimationUsingKeyFrames>
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                        <EventTrigger RoutedEvent="ToggleButton.Unchecked" SourceName="ShowViewFinder">
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimationUsingKeyFrames AccelerationRatio="1"
                                                                   BeginTime="00:00:00"
                                                                   Storyboard.TargetName="LayoutScaleTransform"
                                                                   Storyboard.TargetProperty="(ScaleTransform.ScaleY)">
                                        <SplineDoubleKeyFrame KeySpline="0.5,0.5,0.5,0.5"
                                                              KeyTime="00:00:00.3"
                                                              Value="0" />
                                    </DoubleAnimationUsingKeyFrames>
                                    <DoubleAnimationUsingKeyFrames AccelerationRatio="1"
                                                                   BeginTime="00:00:00"
                                                                   Storyboard.TargetName="LayoutScaleTransform"
                                                                   Storyboard.TargetProperty="(ScaleTransform.ScaleX)">
                                        <SplineDoubleKeyFrame KeySpline="0.5,0.5,0.5,0.5"
                                                              KeyTime="00:00:00.3"
                                                              Value="0" />
                                    </DoubleAnimationUsingKeyFrames>
                                    <DoubleAnimation DecelerationRatio="1"
                                                     Duration="0:0:0.3"
                                                     FillBehavior="HoldEnd"
                                                     From="1"
                                                     Storyboard.TargetName="ViewFinder"
                                                     Storyboard.TargetProperty="Opacity"
                                                     To="0" />
                                    <DoubleAnimation DecelerationRatio="1"
                                                     Duration="0:0:0.3"
                                                     FillBehavior="HoldEnd"
                                                     From="0"
                                                     Storyboard.TargetName="ShowViewFinder"
                                                     Storyboard.TargetProperty="Opacity"
                                                     To="1" />
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Background" Value="White" />
        <Setter Property="ZoomBoxBackground">
            <Setter.Value>
                <LinearGradientBrush po:Freeze="true" StartPoint="0.0, 0.0" EndPoint="1.0, 1.0">
                    <GradientStop Offset="0.0" Color="Silver" />
                    <GradientStop Offset="1.0" Color="DarkGray" />
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
        <Setter Property="ZoomBoxBorderBrush" Value="Black" />
        <Setter Property="ZoomBoxBorderThickness" Value="1" />
        <Setter Property="ClipToBounds" Value="True" />
        <Style.Triggers>
            <Trigger Property="ModifierMode" Value="Pan">
                <Setter Property="Cursor" Value="SizeAll" />
            </Trigger>
            <Trigger Property="ModifierMode" Value="ZoomBox">
                <Setter Property="Cursor" Value="Hand" />
            </Trigger>
        </Style.Triggers>
    </Style>


